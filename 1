# 1- assigning elements to different lists

dict1 = { 1:"Hello",2:"World",3:"it's"}
dict1[4] = "2020"

print(dict1)


\\\\\\\\\\\\\\\\\


# 2 - accessing elements from a tuple
data = ("FRENCH", "GERMAN", "HINDI","SPANISH","HEBREW")

print(data)

print(data[1])

print(data[4])


\\\\\\\\\\\\\\\\


#3- deleting different dictionary elements

d1 = { 1:"carrot", 2:"turnip", 3:"cabbage", 4:"capsicum", 5:"potato", 6:"onion"}
del d1[2]
 
print(d1)

